.1
#include<iostream>
#include<string>
using namespace std;
int main()
{
    string n;
    getline(cin,n);
    cout<<n.length();
}
Q.2

#include<bits/stdc++.h>
using namespace std;
int main()
{
    string n;
    getline(cin,n);
    char occurences;
    cin>>occurences;
    int k=count(n.begin(),n.end(),occurences);
    cout<<k;
}
Q.3
#include<bits/stdc++.h>
using namespace std;
int main()
{
    string n;
    getline(cin,n);
    string m;
    getline(cin,m);
    cout<<n.append(m);
}

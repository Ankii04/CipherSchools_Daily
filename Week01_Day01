//Q. 1

#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    string a;
    cin>>a;
    string b;
    cin>>b;
    cout<<a.append(b);
    return 0;
}


//Q.2

#include<iostream>
using namespace std;
int main()
{
    int n=5;
    int grade;
    float sum=0;
    for(int i=1;i<=n;i++)
    {
    cout<<"Enter grade "<<i<<": ";
    cin>>grade;
    sum+=grade;
    }
    float average= sum/5;
    cout<<average;
    return 0;
}

